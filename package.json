{
  "name": "infiniplayer",
  "version": "0.0.1",
  "description": "An infinite stream of music, in your Terminal.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "music",
    "youtube",
    "player",
    "music-player",
    "media-player",
    "typescript"
  ],
  "bin": {
    "infiniplayer": "./lib/index.js"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "prepare": "npm run build",
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mat-sz/infiniplayer.git"
  },
  "bugs": {
    "url": "https://github.com/mat-sz/infiniplayer/issues"
  },
  "homepage": "https://github.com/mat-sz/infiniplayer",
  "author": "mat-sz <contact@matsz.dev>",
  "license": "BSD-3-Clause-Clear",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/uuid": "^7.0.0",
    "@types/blessed": "^0.1.17",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "command-line-args": "^5.1.1",
    "ffbinaries": "^1.1.4",
    "fluent-ffmpeg": "^2.1.2",
    "media-api": "^0.0.6",
    "speaker": "^0.5.2"
  }
}
